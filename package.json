    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.7.0"
  },
  "dependencies": {
    "better-sqlite3": "^5.0.1",
    "bufferutil": "^3.0.3",
    "discord-akairo": "^7.5.4",
    "discord.js": "^11.3.2",
    "dotenv": "^5.0.1",
    "erlpack": "^0.1.2",
